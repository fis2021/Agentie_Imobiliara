<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="11" prefHeight="500" prefWidth="700" vgap="11" xmlns="http://javafx.com/javafx/11.0.1"
          xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="org.loose.fis.sre.controllers.Open_app_register">
    <padding>
        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
    </padding>
    <Text id="title-text" text="Agentie Imobiliara" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="0" />
    <HBox alignment="bottom_right" prefHeight="54.0" prefWidth="162.0" spacing="10" GridPane.columnIndex="0" GridPane.rowIndex="2">
        <Button onAction="#handleOpenRegister" prefHeight="59.0" prefWidth="164.0" text="Register" />
    </HBox>
    <HBox alignment="bottom_right" prefHeight="54.0" prefWidth="162.0" spacing="10" GridPane.columnIndex="0" GridPane.rowIndex="3">
        <Button onAction="#handleOpenLogin" prefHeight="59.0" prefWidth="164.0" text="Login" />
    </HBox>
    <HBox alignment="bottom_right" prefHeight="54.0" prefWidth="162.0" spacing="10" GridPane.columnIndex="0" GridPane.rowIndex="4">
        <Button onAction="#handleBook" prefHeight="59.0" prefWidth="164.0" text="Book" />
    </HBox>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
